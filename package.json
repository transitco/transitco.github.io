    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.9.0",
    "eslint-plugin-ember": "^8.5.0",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0"
