    "ember-bootstrap": "^3.1.3",
    "ember-cli": "~3.21.2",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^5.2.0",
